{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "run-current",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "run-cli",
      "type": "debugpy",
      "request": "launch",
      "module": "python_workspace.cli",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
